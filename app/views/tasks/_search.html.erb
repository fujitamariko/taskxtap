  <%= search_form_for @q, url: url do |f| %>

    <div style="display:inline-flex">
      <%= f.select :labels_id_eq, Label.pluck(:name, :id), { include_blank: true } %>
      <%= f.search_field :medium_category_cont, class: 'form-control', placeholder: '中分類' %>
      <%= f.search_field :sub_category_cont, class: 'form-control', placeholder: '小分類' %>
      <%= f.search_field :content_cont, class: 'form-control', placeholder: '内容' %>
        <%= f.submit '検索' %>
    </div>

  <% end %>